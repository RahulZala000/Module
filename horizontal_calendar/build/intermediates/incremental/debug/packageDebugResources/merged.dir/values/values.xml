<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <color name="black">#FF000000</color>
    <color name="btnColorGreen">@color/colorMint</color>
    <color name="btnColorWhite">@color/white</color>
    <color name="colorAccent">@color/textColorGray</color>
    <color name="colorBlueLight">#6EC8E6</color>
    <color name="colorBlueSky">#288CC8</color>
    <color name="colorCarmin">#F03C5A</color>
    <color name="colorGold">#E6DC6E</color>
    <color name="colorGrayDark">#222426</color>
    <color name="colorGrayLight">#54565A</color>
    <color name="colorGreen">#28B478</color>
    <color name="colorLime">#A5DC4B</color>
    <color name="colorMint">#6EE6C8</color>
    <color name="colorOrange">#F0A032</color>
    <color name="colorPrimary">@color/colorMint</color>
    <color name="colorPrimaryDark">@color/colorMint</color>
    <color name="colorPurple">#785AB4</color>
    <color name="colorViolet">#DC5AA0</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="textColorGray">#3E414E</color>
    <color name="white">#FFFFFFFF</color>
    <string name="app_name">demo_module</string>
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:forceDarkAllowed" ns1:targetApi="q">false</item>
    </style>
    <style name="AppThemeStatusBar" parent="AppTheme">
        
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="colorPrimaryDark">@color/white</item>
    </style>
    <style name="TextView_Medium">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textDirection">locale</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/textColorGray</item>
        <item name="android:fontFamily">@font/montserrat_medium</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="TextView_Small">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textDirection">locale</item>
        <item name="android:textColor">@color/textColorGray</item>
        <item name="android:fontFamily">@font/montserrat_medium</item>
        <item name="android:textSize">12sp</item>
        <item name="android:ellipsize">end</item>
    </style>
    <declare-styleable name="RowCalendarView">
        
        <attr format="color" name="textColorNormal"/>
        
        <attr format="color" name="colorDayText"/>
        
        <attr format="color" name="colorDateText"/>

        
        <attr format="color" name="textColorSelected"/>
        
        <attr format="color" name="colorDayTextSelected"/>
        
        <attr format="color" name="colorDateTextSelected"/>
        
        <attr format="reference" name="selectedDateBackground"/>

        
        <attr format="color" name="selectorColor"/>

        
        <attr format="dimension" name="sizeDayText"/>
        
        <attr format="dimension" name="sizeDateText"/>

    </declare-styleable>
</resources>